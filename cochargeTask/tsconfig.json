{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "module": "ESNext",
    "target": "ESNext",
    "jsx": "react-native",
    "typeRoots": [
      "node_modules/@types",
      "./src/types"
    ]
  },
  "include": [
    "src/*"
  ],
  "extends": "expo/tsconfig.base"
}
